  "license": "MIT",
  "scripts": {
    "test": "./node_modules/.bin/jest",
    "email:compile": "./node_modules/.bin/tsc ./src/Tools/template-builder.ts",
    "email:run-sync": "node ./src/Tools/template-builder.js",
    "email:sync": "yarn email:compile && yarn email:run-sync",
    "start": "NODE_PATH=./dist concurrently 'yarn email:sync' 'node ./dist/server.js'",
    "dev": "NODE_PATH=./src node_modules/.bin/nodemon -L --exec ts-node src/server.ts",
    "build": "NODE_PATH=./src node_modules/.bin/tsc -p tsconfig.production.json",
    "migrate": "yarn sequelize-cli db:migrate",
    "migrate:undo": "yarn sequelize-cli db:migrate:undo",
    "seed": "yarn sequelize-cli db:seed:all",
    "seed:undo": "yarn sequelize-cli db:seed:undo:all"
  },
  "dependencies": {
    "@sentry/integrations": "^5.15.5",
    "@sentry/node": "5.15.5",
    "@slack/web-api": "^5.8.1",
