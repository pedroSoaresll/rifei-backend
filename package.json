    "eslint-plugin-jest": "^23.9.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.0.1",
    "mongodb-memory-server": "^6.5.2",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
