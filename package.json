    "cheerio": "^1.0.0-rc.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mailgun-js": "^0.22.0",
    "mjml": "4.6.3",
    "moment": "^2.25.3",
    "mongoose": "5.9.12",
    "mysql2": "^2.1.0",
    "node-cron": "^2.0.3",
    "nunjucks": "3.2.1",
    "request": "^2.88.2",
    "sequelize": "^6.3.1",
