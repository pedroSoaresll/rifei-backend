    "typescript": "^3.8.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@sucrase/jest-plugin": "^2.0.0",
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "4.17.6",
    "@types/jest": "25.2.1",
    "@types/mailgun-js": "^0.22.4",
    "@types/mjml": "^4.0.3",
    "@types/moment": "^2.13.0",
    "@types/mongoose": "^5.7.15",
    "@types/node": "^14.0.22",
    "@types/node-cron": "^2.0.3",
    "@types/nunjucks": "^3.1.3",
    "@types/request": "^2.48.4",
    "@types/sequelize": "^4.28.9",
    "@types/validator": "^13.1.0",
